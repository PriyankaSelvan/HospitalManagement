<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

          <hibernate-mapping>
          <class name="models.Prescription" table="Prescription">
          <id name="prescription_id">
          <generator class="assigned"></generator>
          </id>
          
          
          
          <property name="patient_id" type = "int" column="Patient"/>
          <property name="current_temperature" type = "double" column="CurrentTemperature"/>
          <property name="current_pressure" type = "double" column="CurrentPressure"/>
          <property name="inventory_id_1" type = "int" column = "Inventory1"/>
          <property name="inventory_id_2" type = "int" column = "Inventory2"/>
          <property name="inventory_id_3" type = "int" column = "Inventory3"/>
          <property name="inventory_id_4" type = "int" column = "Inventory4"/>
          <property name="inventory_id_5" type = "int" column = "Inventory5"/>
          
          </class>
          
          </hibernate-mapping>